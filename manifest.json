{
  "manifest_version": 3,
  "name": "Muda",
  "version": "1.0.0",
  "description": "Muda Chrome extension",
  "action": {
    "default_icon": {
      "16": "icons/logo-bg-stars.png",
      "24": "icons/logo-bg-stars.png",
      "32": "icons/logo-bg-stars.png"
    },
    "default_title": "Click Me",
    "default_popup": "html/popup.html"
  },
  "icons": {
    "16": "icons/logo-bg-stars.png",
    "48": "icons/logo-bg-stars.png",
    "128": "icons/logo-bg-stars.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*",
        "https://www.instagram.com/*"
      ],
      "css": [
        "css/block-site-style.css"
      ]
    }
  ]
}